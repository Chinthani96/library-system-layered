<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MembersController" stylesheets="/css/application.css">
   <children>
      <Button fx:id="btnAdd" layoutX="24.0" layoutY="21.0" mnemonicParsing="false" onAction="#btnAdd_OnAction" prefHeight="20.0" prefWidth="212.0" text="Add new member" />
      <Button fx:id="btnBack" layoutX="531.0" layoutY="21.0" mnemonicParsing="false" onAction="#btnBack_OnAction" text="Back" />
      <TextField fx:id="txtMemberId" editable="false" layoutX="24.0" layoutY="65.0" prefHeight="27.0" prefWidth="553.0" promptText="Member ID" />
      <TextField fx:id="txtMemberName" layoutX="24.0" layoutY="99.0" prefHeight="27.0" prefWidth="553.0" promptText="Name" />
      <TextField fx:id="txtAddress" layoutX="24.0" layoutY="133.0" prefHeight="27.0" prefWidth="553.0" promptText="Address" />
      <TableView fx:id="tblMembers" layoutX="24.0" layoutY="203.0" prefHeight="183.0" prefWidth="553.0">
        <columns>
          <TableColumn prefWidth="178.0" text="Member ID" />
          <TableColumn prefWidth="174.0" text="Name" />
            <TableColumn prefWidth="200.0" text="Address" />
        </columns>
      </TableView>
      <Button fx:id="btnDelete" layoutX="523.0" layoutY="166.0" mnemonicParsing="false" onAction="#btnDelete_OnAction" text="Delete" />
      <Button fx:id="btnSave" layoutX="465.0" layoutY="166.0" mnemonicParsing="false" onAction="#btnSave_OnAction" text="Save" />
   </children>
</AnchorPane>
